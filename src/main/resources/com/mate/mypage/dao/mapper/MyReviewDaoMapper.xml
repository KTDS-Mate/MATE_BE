<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mate.mypage.dao.MyReviewDao">

<select id="selectMyReviewAllCount" resultType="_int">
	SELECT COUNT(1)
	  FROM GD_TR_RVW
</select>

<select id="selectAllMyReview" resultType="com.mate.mypage.vo.MyReviewVO">
	SELECT GD_TR_RVW_ID
		 , ATHR_ID
		 , GD_TR_PST_ID
		 , GD_TR_RVW_TTL
		 , GD_TR_RVW_CNTNT
		 , GD_TR_RVW_CRTDAT
		 , GD_TR_RVW_DLT_AT
		 , GD_TR_RVW_IS_DLT
		 , GD_TR_RVW_RTNG
	  FROM GD_TR_RVW
</select>




</mapper>